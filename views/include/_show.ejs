<% for(var i=0;i<users.length;i++){ %>
    <tr>
        <td>
            <%=i+1%>
        </td>
        <td>
            <%=users[i].name %>
        </td>
        <td>
            <%=users[i].email%>
        </td>
        <td>
            <%=users[i].gender%>
        </td>
        <td>
            <%=users[i].status%>
        </td>
        <td>
            <a href="/update-user/<%=users[i]._id%>" class=" border-shadow update btn">
                <span class="text-gradient"><i class="fa-solid fa-pencil"></i></span>
            </a>
            <a class=" border-shadow btn delete-item" data-id=<%=users[i]._id%>>
                <span class="text-gradient"><i class="fa-solid fa-trash-can"></i></span>
            </a>
        </td>
    </tr>
    <%}%>